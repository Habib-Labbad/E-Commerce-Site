{"ast":null,"code":"import { computed } from \"vue\";\nimport { cartItems } from \"../fake-data\";\nexport default {\n  name: \"ProductsPage\",\n  data() {\n    return {\n      cartItems\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.cartItems.reduce((sum, item) => sum + Number(item.price), 0);\n    }\n  }\n};","map":{"version":3,"names":["computed","cartItems","name","data","totalPrice","reduce","sum","item","Number","price"],"sources":["src/views/CartPage.vue"],"sourcesContent":["<template>\n  <div id=\"page-wrap\">\n    <h1>Shopping Cart</h1>\n    <div\n      v-for=\"product in cartItems\"\n      :key=\"product.id\"\n      class=\"product-container\"\n    >\n      <img class=\"product-image\" :src=\"product.imageUrl\" alt=\"\" />\n      <div class=\"details-wrap\">\n        <h3>{{ product.name }}</h3>\n        <p>${{ product.price }}</p>\n      </div>\n      <button class=\"remove-button\">Remove from Cart</button>\n    </div>\n    <h3 id=\"total-price\">Total: ${{ totalPrice }}</h3>\n    <button id=\"checkout-button\">Proceed to Checkout</button>\n  </div>\n</template>\n\n<script>\nimport { computed } from \"vue\";\nimport { cartItems } from \"../fake-data\";\nexport default {\n  name: \"ProductsPage\",\n  data() {\n    return {\n      cartItems,\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.cartItems.reduce((sum, item) => sum + Number(item.price), 0);\n    },\n  },\n};\n</script>\n\n<style>\nh1 {\n  border-bottom: 1px solid black;\n  margin: 0;\n  margin-top: 16px;\n  padding: 16px;\n}\n#total-price {\n  padding: 16px;\n  text-align: right;\n}\n#checkout-button {\n  width: 100%;\n}\n</style>"],"mappings":"AAqBA,SAAAA,QAAA;AACA,SAAAC,SAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAF;IACA;EACA;EACAD,QAAA;IACAI,WAAA;MACA,YAAAH,SAAA,CAAAI,MAAA,EAAAC,GAAA,EAAAC,IAAA,KAAAD,GAAA,GAAAE,MAAA,CAAAD,IAAA,CAAAE,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}